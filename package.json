{
  "name": "spirom",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*",
    "sanity/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "dev:web": "npm run dev --workspace=apps/web",
    "dev:sanity": "npm run dev --workspace=sanity/studio",
    "build:web": "npm run build --workspace=apps/web",
    "build:sanity": "npm run build --workspace=sanity/studio"
  },
  "devDependencies": {
    "turbo": "^2.3.0"
  },
  "overrides": {
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0"
  },
  "packageManager": "npm@10.8.2"
}
